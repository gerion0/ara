{
	"steps": [
		"InteractionAnalysis"
	],

	"POSIXInit": {
		"system_profile": "POSIX",
		"enable_musl_syscalls": true
	},

	"InteractionAnalysis": {
		"dump": true,
		"count_syscalls": true,
		"log_level": "info"
	},

	"RemoveSysfuncBody": {
		"log_level": "debug",
		"drop_llvm_suffix": false
	},

	"SysFuncts": {
		"no_stubs": true
	},

	"SystemRelevantFunctions": {
		"dump": true,
		"no_stubs": true
	},

	"CallGraph": {
		"dump": true
	},

	"ResolveFunctionPointer": {
		"log_level": "debug"
	},

    "IRReader": {
        "no_sysfunc_body": true
    },

    "RecursiveFunctions": {
        "no_recursive_funcs": false
    },

	"ICFG": {
		"dump": true
	},

	"logger": {
		"POSIX": "debug"
	}
}
